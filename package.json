{
  "name": "react-router-url-params",
  "version": "0.9.2",
  "description": "Strongly-typed way to manage URL parameters in react-router",
  "homepage": "https://github.com/mcctomsk/react-router-url-params#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/mcctomsk/react-router-url-params.git"
  },
  "license": "MIT",
  "author": "Artur Drobinskiy",
  "main": "dist/index.js",
  "module": "dist/react-router-url-params.esm.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "react",
    "react-router",
    "url",
    "query",
    "parameters",
    "hook",
    "hooks",
    "query param",
    "react use"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "cd example && yarn start",
    "do-publish": "yarn typecheck && yarn lint && yarn test && yarn publish",
    "typecheck": "tsc",
    "build": "dts build",
    "analyze": "size-limit --why",
    "build-storybook": "build-storybook",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "storybook": "start-storybook -p 6006",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16.8.0",
    "react-router": ">=6.3.0",
    "react-router-dom": ">=6.3.0"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/react-router-url-params.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-router-url-params.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@tsconfig/create-react-app": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "dts-cli": "^1.5.2",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router": "6.14.0-pre.0",
    "react-router-dom": "6.14.0-pre.0",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "query-string": "^7.1.3",
    "serialize-query-params": "^2.0.1"
  }
}
